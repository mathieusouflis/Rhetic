{
  "kind": "collectionType",
  "collectionName": "up_users",
  "info": {
    "name": "user",
    "description": "",
    "singularName": "user",
    "pluralName": "users",
    "displayName": "User"
  },
  "options": {
    "draftAndPublish": false
  },
  "attributes": {
    "username": {
      "type": "string",
      "minLength": 3,
      "unique": true,
      "configurable": false,
      "required": true
    },
    "email": {
      "type": "email",
      "minLength": 6,
      "configurable": false,
      "required": true
    },
    "provider": {
      "type": "string",
      "configurable": false
    },
    "password": {
      "type": "password",
      "minLength": 6,
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "resetPasswordToken": {
      "type": "string",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "confirmationToken": {
      "type": "string",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "confirmed": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "blocked": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "role": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.role",
      "inversedBy": "users",
      "configurable": false
    },
    "avatar": {
      "allowedTypes": [
        "images"
      ],
      "type": "media",
      "multiple": false,
      "required": false
    },
    "bio": {
      "type": "text"
    },
    "last_login_at": {
      "type": "datetime"
    },
    "is_active": {
      "type": "boolean"
    },
    "email_verified": {
      "type": "boolean"
    },
    "two_factor_enabled": {
      "type": "boolean"
    },
    "posts": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::post.post",
      "mappedBy": "author"
    },
    "comments": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::comment.comment",
      "mappedBy": "author"
    },
    "moderatedSubrhetics": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::subrhetic.subrhetic",
      "mappedBy": "moderators"
    },
    "createdSubrhetics": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::subrhetic.subrhetic",
      "mappedBy": "creator"
    },
    "votes": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::vote.vote",
      "mappedBy": "user"
    },
    "joined_subrhetics": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::subrhetic.subrhetic",
      "mappedBy": "members"
    },
    "banned_from_subrhetics": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::subrhetic.subrhetic",
      "mappedBy": "banned_users"
    },
    "subrhetic_topics": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::subrhetic-topic.subrhetic-topic",
      "mappedBy": "users_permissions_user"
    },
    "subrhetic_emojis": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::subrhetic-emoji.subrhetic-emoji",
      "mappedBy": "users_permissions_user"
    },
    "user_preference": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::user-preference.user-preference",
      "inversedBy": "users_permissions_user"
    },
    "subrhetic_emoji_reactions": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::subrhetic-emoji-reaction.subrhetic-emoji-reaction",
      "mappedBy": "users_permissions_user"
    },
    "notifications": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::notification.notification",
      "mappedBy": "users_permissions_user"
    },
    "anonymous_post_authors": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::anonymous-post-author.anonymous-post-author",
      "mappedBy": "users_permissions_user"
    },
    "post_flair_assignments": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::post-flair-assignment.post-flair-assignment",
      "mappedBy": "users_permissions_user"
    },
    "user_flair_assignments": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::user-flair-assignment.user-flair-assignment",
      "mappedBy": "users_permissions_user"
    },
    "saved_items": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::saved-item.saved-item",
      "mappedBy": "users_permissions_user"
    },
    "report": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::report.report",
      "mappedBy": "reporter"
    },
    "reports": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::report.report",
      "mappedBy": "reported_user"
    },
    "subrhetic_rules": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::subrhetic-rule.subrhetic-rule",
      "mappedBy": "made_by"
    },
    "user_activity_logs": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::user-activity-log.user-activity-log",
      "mappedBy": "users_permissions_user"
    },
    "user_blocks": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::user-block.user-block",
      "mappedBy": "blocked"
    },
    "private_messages": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::private-message.private-message",
      "mappedBy": "recipient"
    },
    "user_online_status": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::user-online-status.user-online-status",
      "mappedBy": "users_permissions_user"
    },
    "moderation_actions": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::moderation-action.moderation-action",
      "mappedBy": "users_permissions_user"
    },
    "fav_subrhetics": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::subrhetic.subrhetic",
      "mappedBy": "fav_users"
    }
  }
}
